    <asp:SqlDataSource runat="server" 
        ConnectionString="<%$ ConnectionStrings:MucdonghiencuuConnectionString1 %>" 
        DeleteCommand="DELETE FROM [Dientichchitiet] WHERE [ID_DTCT] = @ID_DTCT" 
        InsertCommand="INSERT INTO [Dientichchitiet] ([KHLT], [Banvechitiet], [Tendientich], [Vitridialy], [Dientich], [Tyle], [Mucdo], [Mucdich], [Doituongdieutra], [Tenbanve], [Anh], [geo]) VALUES (@KHLT, @Banvechitiet, @Tendientich, @Vitridialy, @Dientich, @Tyle, @Mucdo, @Mucdich, @Doituongdieutra, @Tenbanve, @Anh, @geo)" 
        ProviderName="<%$ ConnectionStrings:MucdonghiencuuConnectionString1.ProviderName %>" 
        SelectCommand="SELECT * FROM [Dientichchitiet]" 
        UpdateCommand="UPDATE [Dientichchitiet] SET [Banvechitiet] = @Banvechitiet, [Tendientich] = @Tendientich, [Vitridialy] = @Vitridialy, [Dientich] = @Dientich, [Tyle] = @Tyle, [Mucdo] = @Mucdo, [Mucdich] = @Mucdich, [Doituongdieutra] = @Doituongdieutra, [Tenbanve] = @Tenbanve, [Anh] = @Anh, [geo] = @geo WHERE [ID_DTCT] = @ID_DTCT" 
        FilterExpression="KHLT= '{0}'"
        ID="SqlDataDTCT">
        <FilterParameters>
            <asp:ControlParameter ControlID="drDeAn" Name="KHLT" PropertyName="SelectedValue" />
        </FilterParameters>
        <DeleteParameters>
            <asp:Parameter Name="ID_DTCT" Type="Int32"></asp:Parameter>
        </DeleteParameters>
        <InsertParameters>
            <asp:Parameter Name="KHLT" Type="String"></asp:Parameter>
            <asp:Parameter Name="Banvechitiet" Type="String"></asp:Parameter>
            <asp:Parameter Name="Tendientich" Type="String"></asp:Parameter>
            <asp:Parameter Name="Vitridialy" Type="String"></asp:Parameter>
            <asp:Parameter Name="Dientich" Type="Double"></asp:Parameter>
            <asp:Parameter Name="Tyle" Type="String"></asp:Parameter>
            <asp:Parameter Name="Mucdo" Type="String"></asp:Parameter>
            <asp:Parameter Name="Mucdich" Type="String"></asp:Parameter>
            <asp:Parameter Name="Doituongdieutra" Type="String"></asp:Parameter>
            <asp:Parameter Name="Tenbanve" Type="String"></asp:Parameter>
            <asp:Parameter Name="Anh" Type="String"></asp:Parameter>
            <asp:Parameter Name="geo" Type="Object"></asp:Parameter>
        </InsertParameters>
        <UpdateParameters>
            <asp:Parameter Name="KHLT" Type="String"></asp:Parameter>
            <asp:Parameter Name="Banvechitiet" Type="String"></asp:Parameter>
            <asp:Parameter Name="Tendientich" Type="String"></asp:Parameter>
            <asp:Parameter Name="Vitridialy" Type="String"></asp:Parameter>
            <asp:Parameter Name="Dientich" Type="Double"></asp:Parameter>
            <asp:Parameter Name="Tyle" Type="String"></asp:Parameter>
            <asp:Parameter Name="Mucdo" Type="String"></asp:Parameter>
            <asp:Parameter Name="Mucdich" Type="String"></asp:Parameter>
            <asp:Parameter Name="Doituongdieutra" Type="String"></asp:Parameter>
            <asp:Parameter Name="Tenbanve" Type="String"></asp:Parameter>
            <asp:Parameter Name="Anh" Type="String"></asp:Parameter>
            <asp:Parameter Name="geo" Type="Object"></asp:Parameter>
